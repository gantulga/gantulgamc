{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-belongs-to-parent-field": "cd nova-components/BelongsToParentField && npm run dev",
        "build-belongs-to-parent-field-prod": "cd nova-components/BelongsToParentField && npm run prod",
        "build-settings": "cd nova-components/Settings && npm run dev",
        "build-settings-prod": "cd nova-components/Settings && npm run prod",
        "build-media-tool": "cd nova-components/MediaTool && npm run dev",
        "build-media-tool-prod": "cd nova-components/MediaTool && npm run prod",
        "build-media-field": "cd nova-components/MediaField && npm run dev",
        "build-media-field-prod": "cd nova-components/MediaField && npm run prod",
        "build-belongs-to-many-checkbox": "cd nova-components/BelongsToManyCheckbox && npm run dev",
        "build-belongs-to-many-checkbox-prod": "cd nova-components/BelongsToManyCheckbox && npm run prod",
        "build-block-field": "cd nova-components/BlockFields && npm run dev",
        "build-block-field-prod": "cd nova-components/BlockFields && npm run prod",
        "build-rating-card": "cd nova-components/RatingCard && npm run dev",
        "build-rating-card-prod": "cd nova-components/RatingCard && npm run prod",
        "build-editor": "cd nova-components/Editor && npm run dev",
        "build-editor-prod": "cd nova-components/Editor && npm run prod"
    },
    "devDependencies": {
        "@ky-is/tailwind-color-palette": "^0.1.1",
        "axios": "^0.18",
        "cross-env": "^5.1",
        "laravel-mix": "^2.1",
        "laravel-mix-purgecss": "^2.2",
        "laravel-mix-tailwind": "^0.1.0",
        "laravel-nova": "^1.0.9",
        "lodash": "^4.17.21",
        "popper.js": "^1.12",
        "tailwindcss": "^0.7.2",
        "tailwindcss-alpha": "^0.1.4",
        "tailwindcss-gradients": "^1.1.0",
        "vue": "^2.6.4"
    },
    "dependencies": {
        "form-backend-validation": "^2.3.9",
        "scrollreveal": "^4.0.5",
        "turbolinks": "^5.2.0",
        "vue-carousel": "0.17.0",
        "vue-flatpickr-component": "^8.1.5",
        "vue-perfect-scrollbar": "^0.1.0",
        "vue-progressbar": "^0.7.5",
        "vue-text-mask": "^6.1.2",
        "vue-turbolinks": "^2.0.4",
        "vuejs-datepicker": "^1.6.2"
    }
}
